<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="646.0" prefWidth="957.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bsu.comp152.Controller">
   <children>
      <HBox layoutX="6.0" layoutY="6.0" prefHeight="637.0" prefWidth="948.0">
         <children>
            <VBox prefHeight="637.0" prefWidth="313.0">
               <children>
                  <Label prefHeight="52.0" prefWidth="237.0" text="Choose Recipe Type:" textFill="#bf3939">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <MenuButton mnemonicParsing="false" text="Recipe Types:">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="Casserole" />
                      <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="Burger" />
                        <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="Vegetable" />
                        <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="Cake" />
                        <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="Cookie" />
                        <MenuItem mnemonicParsing="false" onAction="#selectMenuItem" text="Pasta" />
                    </items>
                  </MenuButton>
                  <Button mnemonicParsing="false" onAction="#loadData" prefHeight="68.0" prefWidth="150.0" text="Get Recipies" />
               </children>
            </VBox>
            <VBox prefHeight="637.0" prefWidth="318.0">
               <children>
                  <CheckBox mnemonicParsing="false" text="Mushrooms" fx:id="mushrooms" />
                  <CheckBox mnemonicParsing="false" text="Cheese" fx:id="cheese"/>
                  <CheckBox mnemonicParsing="false" text="Kale" fx:id="kale"/>
                  <CheckBox mnemonicParsing="false" text="Chicken" fx:id="chicken"/>
                  <CheckBox mnemonicParsing="false" text="rice" fx:id="rice"/>
                  <CheckBox mnemonicParsing="false" text="flour" fx:id="flour"/>
               </children></VBox>
            <VBox prefHeight="637.0" prefWidth="316.0">
               <children>
                  <ListView fx:id="ListControl" prefHeight="638.0" prefWidth="316.0" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
